
clear all
%Sin(i)
precision=0.0001; %Smaller better
x = 0:precision:pi()/2;
comp = 100000;
ranset = zeros(comp,1);
for i = 1:comp
    r = randi(length(x));
    ranset(i,1)=sin(x(r));
end
m = mean(ranset);
s = std(ranset);
hist(ranset,DoanesBins(ranset,2))

testmass = 10;
for i = 1:comp
    r = randi(length(ranset));
    testmasses(i) = testmass/r